{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": true,
        "preserveConstEnums": true,
        "removeComments": true,
        "sourceMap": false,
        "inlineSources": false
    },
    "include": [
        "src/**/*",
        "typings/**/*"
    ],
    "exclude": [
        "src/libs",
        "wwwroot"
        "node_modules"
    ]
}
{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": true,
        "preserveConstEnums": true,
        "removeComments": true,
        "sourceMap": false,
        "inlineSources": false,
        "rootDir": "src",
        "outDir": "wwwroot"
    },
    "include": [
        "src/**/*",
        "typings/**/*"
    ],
    "exclude": [
        "src/libs",
        "wwwroot",
        "node_modules"
    ]
}
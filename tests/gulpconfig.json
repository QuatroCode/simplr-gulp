{
    "Directories": {
        "Source": "src",
        "Build": "wwwroot",
        "App": "app"
    },
    "TypeScriptConfig": {
        "Development": "tsconfig.json",
        "Production": "tsconfig.production.json"
    },
    "ServerConfig": {
        "Ip": "127.0.0.1",
        "Port": 4000,
        "LiveReloadPort": 4400
    },
    "BundleConfig": {
        "AppFile": "app.js",
        "BuildFile": "build.js",
        "Include": [
          "css"
        ],
        "Exclude": [
            "[wwwroot/js/app/**/*]"
        ]
    },
    "ExtensionsMap": {
        "ts": "js",
        "tsx": "js",
        "scss": "css"
    },
    "WebConfig": "web.config",
    "CfgVersion": 2.01
}
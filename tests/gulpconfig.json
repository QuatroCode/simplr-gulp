{
    "Directories": {
        "Source": "src",
        "Build": "wwwroot",
        "App": "app"
    },
    "TypeScriptConfig": {
        "Development": "tsconfig.json",
        "Production": "tsconfig.production.json"
    },
    "BundleConfig": {
        "AppFile": "app.js",
        "BuildFile": "build.js",
        "Include": [],
        "Exclude": [
            "[wwwroot/js/app/**/*]"
        ]
    },
    "ExtensionsMap": {
        "ts": "js",
        "tsx": "js",
        "scss": "css"
    },
    "WebConfig": "web.config",
    "ServerPort": 4000,
    "LiveReloadPort": 4400,
    "ServerIp": "127.0.0.1",
    "CfgVersion": 2.01
}